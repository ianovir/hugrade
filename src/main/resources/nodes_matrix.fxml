<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import com.ianovir.hugrade.views.TransitionMatrixView?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TableView?>
<BorderPane xmlns="http://javafx.com/javafx" fx:id="mainPane" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ianovir.hugrade.controllers.NodesMatrixController"
            prefHeight="400.0" prefWidth="600.0">
    <top>
        <AnchorPane prefHeight="0.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <MenuBar AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <Menu mnemonicParsing="false" text="Export">
                    <MenuItem fx:id="miTxExportTxt" mnemonicParsing="false" text=".txt" disable="true"/>
                    <MenuItem fx:id="miTxExportCsv" mnemonicParsing="false" text=".csv" disable="true" />
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <MenuItem mnemonicParsing="false" text="About" />
                </Menu>
            </MenuBar>
        </AnchorPane>
    </top>
    <right/>
    <bottom/>
    <left/>
    <center>
        <AnchorPane>
            <TableView fx:id="nodesMatrix" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                       AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
    </center>

</BorderPane>
